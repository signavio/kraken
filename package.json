{
  "name": "@signavio/kraken",
  "version": "5.1.1",
  "description": "Load API entities",
  "repository": "git@github.com:signavio/kraken.git",
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf lib lib-es6",
    "build": "yarn build:es5 && yarn build:es6",
    "build:es5": "BABEL_ENV=es5 babel src --out-dir lib",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir lib-es6",
    "prebuild": "yarn run clean",
    "lint": "eslint src test",
    "test": "mocha",
    "coverage":
      "istanbul cover _mocha --report cobertura --include-all-sources --x \"**/lib/**\"",
    "prepublishOnly": "yarn build",
    "renovate": "renovate",
    "flow": "flow check"
  },
  "author": "Jan-Felix Schwarz",
  "license": "UNLICENSED",
  "dependencies": {
    "hoist-non-react-statics": "^1.2.0",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "reduce-reducers": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "cheerio": "0.22.0",
    "enzyme": "2.7.1",
    "eslint": "4.17.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-flowtype": "2.43.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.6.1",
    "flow-bin": "0.67.1",
    "install": "0.8.4",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
    "normalizr": "3.1.0",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.5",
    "redux": "3.7.1",
    "redux-saga": "0.15.4",
    "renovate": "11.36.2",
    "rimraf": "2.6.1",
    "sinon": "1.17.7",
    "sinon-chai": "2.14.0",
    "xunit-file": "1.0.0"
  },
  "peerDependencies": {
    "normalizr": "^3.1.0",
    "react": ">=15.0.0 || >=16.0.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-saga": ">=0.14.3"
  }
}
